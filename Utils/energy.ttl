# baseURI: http://bim4ren.eu/utils/energy#
# imports: http://www.bim4ren.eu/property/0.1

@prefix : <http://bim4ren.eu/utils/energy#> .
@prefix b4r-prop: <http://www.bim4ren.eu/property#> .
@prefix bs: <https://w3id.org/def/basicsemantics-owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:
  rdf:type owl:Ontology ;
  owl:imports <http://www.bim4ren.eu/property/0.1> ;
  owl:versionIRI <http://www.bim4ren.eu/utils/energy/0.1> ;
.
:Butane
  rdf:type :Gas ;
  rdf:type owl:NamedIndividual ;
  rdfs:label "Butane"@en ;
  rdfs:label "Butane"@fr ;
  rdfs:label "Butano"@es ;
  rdfs:label "Butano"@it ;
.
:Certification
  rdf:type owl:Class ;
.
:Coal
  rdf:type :EnergySource ;
  rdf:type owl:NamedIndividual ;
  rdfs:label "Carbone"@it ;
  rdfs:label "Carbón"@fr ;
  rdfs:label "Charbon"@fr ;
  rdfs:label "Coal"@en ;
.
:Electricity
  rdf:type owl:Class ;
  rdfs:subClassOf :EnergySource ;
.
:EnergyPerformanceCertification
  rdf:type owl:Class ;
  rdfs:subClassOf :Certification ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty :associatedLocation ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :maxEnergyConsumption ;
      owl:someValuesFrom bs:QuantityValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :minEnergyConsumption ;
      owl:someValuesFrom bs:QuantityValue ;
    ] ;
.
:EnergySource
  rdf:type owl:Class ;
.
:Fuel
  rdf:type :EnergySource ;
  rdf:type owl:NamedIndividual ;
  rdfs:label "Fioul"@fr ;
  rdfs:label "Fuel oil"@en ;
  rdfs:label "Fuel"@es ;
.
:GPL
  rdf:type :Gas ;
  rdf:type owl:NamedIndividual ;
  rdfs:label "GPL"@es ;
  rdfs:label "GPL"@fr ;
  rdfs:label "GPL"@it ;
  rdfs:label "Gas di petrolio liquefatti"@it ;
  rdfs:label "Gas licuado de petróleo"@es ;
  rdfs:label "Gaz de pétrole liquéfié"@fr ;
  rdfs:label "LPG"@en ;
  rdfs:label "Liquified Petroleum Gas"@en ;
.
:Gas
  rdf:type owl:Class ;
  rdfs:comment "Gas"@en ;
  rdfs:comment "Gaz"@fr ;
  rdfs:subClassOf :EnergySource ;
.
:NaturalGas
  rdf:type :Gas ;
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Gaz naturel"@fr ;
  rdfs:comment "Natural gas"@en ;
.
:Solar
  rdf:type owl:NamedIndividual ;
  rdfs:label "Solaire"@fr ;
  rdfs:label "Solar"@en ;
  rdfs:label "Solar"@es ;
.
:Wood
  rdf:type :EnergySource ;
  rdf:type owl:NamedIndividual ;
  rdfs:label "Bois"@fr ;
  rdfs:label "Madera"@es ;
  rdfs:label "Wood"@en ;
.
:associatedLocation
  rdf:type owl:DatatypeProperty ;
.
:maxEnergyConsumption
  rdf:type owl:ObjectProperty ;
  rdfs:label "consommation énergétique maximale"@fr ;
  rdfs:label "consumo energetico maximo"@es ;
  rdfs:label "consumo energetico maximo"@it ;
  rdfs:label "maximal energy consumption"@en ;
  rdfs:subPropertyOf :normalizedEnergyConsumption ;
.
:minEnergyConsumption
  rdf:type owl:ObjectProperty ;
  rdfs:label "consommation énergétique minimale"@fr ;
  rdfs:label "consumo energetico minimo"@es ;
  rdfs:label "consumo energetico minimo"@it ;
  rdfs:label "minimal energy consumption"@en ;
  rdfs:subPropertyOf :normalizedEnergyConsumption ;
.
:normalizedEnergyConsumption
  rdf:type owl:ObjectProperty ;
  rdfs:label "consommation énergétique primaire annuelle"@fr ;
  rdfs:label "consumo energetico normalizado"@es ;
  rdfs:label "consumo energetico normalizzato"@it ;
  rdfs:label "normalized energy consumption"@en ;
  rdfs:range bs:QuantityValue ;
.
bs:QuantityValue
  rdf:type owl:Class ;
.
:KiloW-HR-PER-M2-YEAR
        a                 <http://qudt.org/schema/qudt/Unit> , <http://qudt.org/schema/qudt/CGS-Unit> ;
        rdfs:isDefinedBy  <http://qudt.org/2.1/vocab/unit> ;
        rdfs:label        "Kilowatthour per Square Meter per Year" ;
        <http://purl.org/dc/terms/description>
                "The kilowatt hour per square meter per year, is a normalized measure of the energy consumption, noted (\\(kWh \\cdot m^2 \\cdot y))."^^<http://qudt.org/schema/qudt/LatexString> ;
        <http://qudt.org/schema/qudt/expression>
                "\\(kWh \\cdot m^2 \\cdot y\\)"^^<http://qudt.org/schema/qudt/LatexString> ;
        <http://qudt.org/schema/qudt/hasQuantityKind>
                <http://qudt.org/vocab/quantitykind/Energy> .
