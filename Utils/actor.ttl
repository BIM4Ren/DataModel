###  http://www.bim4ren.eu/actors#Actor
b4r-act:Actor rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty b4r-act:workSituation ;
                                owl:allValuesFrom b4r-act:WorkSituation
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty b4r-act:ageCategory ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass b4r-act:AgeCategory
                              ] .


###  http://www.bim4ren.eu/actors#AgeCategory
b4r-act:AgeCategory rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:oneOf ( b4r-act:ADULT
                                                      b4r-act:BABY
                                                      b4r-act:CHILD
                                                      b4r-act:SENIOR
                                                      b4r-act:TEENAGER
                                                      b4r-act:YOUNGSTER
                                                    )
                                        ] .


###  http://www.bim4ren.eu/actors#BuildingManager
b4r-act:BuildingManager rdf:type owl:Class ;
                        rdfs:subClassOf b4r-act:Actor .


###  http://www.bim4ren.eu/actors#BuildingOwner
b4r-act:BuildingOwner rdf:type owl:Class ;
                      rdfs:subClassOf b4r-act:Actor .


###  http://www.bim4ren.eu/actors#OccupancyProfile
b4r-act:OccupancyProfile rdf:type owl:Class ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty b4r-act:associatedZone ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass bot:Zone
                                         ] .


###  http://www.bim4ren.eu/actors#OccupancyTimeRange
b4r-act:OccupancyTimeRange rdf:type owl:Class .


###  http://www.bim4ren.eu/actors#Occupant
b4r-act:Occupant rdf:type owl:Class ;
                 rdfs:subClassOf b4r-act:Actor ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty b4r-act:occupancyProfile ;
                                   owl:allValuesFrom b4r-act:OccupancyProfile
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty b4r-act:occupancyTime ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass b4r-act:OccupancyTimeRange
                                 ] .


###  http://www.bim4ren.eu/actors#WorkSituation
b4r-act:WorkSituation rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty b4r-act:workType ;
                                        owl:allValuesFrom b4r-act:WorkType
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty b4r-act:income ;
                                        owl:allValuesFrom xsd:decimal
                                      ] .


###  http://www.bim4ren.eu/actors#WorkType
b4r-act:WorkType rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( b4r-act:EMPLOYED
                                                   b4r-act:SELF_EMPLOYED
                                                   b4r-act:UNEMPLOYED
                                                 )
                                     ] .


###  https://w3id.org/bot#Zone
bot:Zone rdf:type owl:Class .


