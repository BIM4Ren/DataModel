ex:Window_1
  a buildingelements:Window ;
  b4r-prop:height [
			rdf:value "44.0"^^xsd:decimal ;
			bs:unit unit:CentiM ;
			b4r-prop:accuracy [
						rdf:value "0.9"^^xsd:decimal ;
						] ;
			] ;
.

