###  http://www.bim4ren.eu/domain/electrical#ElectricalConsumer
b4r-elec:ElectricalConsumer rdf:type owl:Class ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty b4r-elec:powerIn ;
                                              owl:allValuesFrom bs:QuantityValue
                                            ] ,
											[ rdf:type owl:Restriction ;
                                              owl:onProperty b4r-elec:electricalEfficiency ;
                                              owl:someValuesFrom bs:QuantityValue
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty b4r-elec:powerOut ;
                                              owl:allValuesFrom bs:QuantityValue
                                            ] .


###  http://www.bim4ren.eu/domain/electrical#ElectricalProducer
b4r-elec:ElectricalProducer rdf:type owl:Class ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty b4r-elec:electricalEfficiency ;
                                              owl:someValuesFrom bs:QuantityValue
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty b4r-elec:powerOut ;
                                              owl:someValuesFrom bs:QuantityValue
                                            ] .


