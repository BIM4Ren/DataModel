#################################################################
#    Classes
#################################################################

###  http://www.bim4ren.eu/domain/thermal#HeatProducer
b4r-thermal:HeatProducer rdf:type owl:Class ;
						 rdfs:subClassOf [ rdf:type owl:Restriction ;
										   owl:onProperty b4r-thermal:thermalEfficiency ;
										   owl:allValuesFrom bs:QuantityValue
										 ] ,
										 [ rdf:type owl:Restriction ;
										   owl:onProperty b4r-thermal:maxTemperature ;
										   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
										   owl:onClass b4r-thermal:HeatProducer
										 ] ,
										 [ rdf:type owl:Restriction ;
										   owl:onProperty b4r-thermal:minTemperature ;
										   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
										   owl:onClass b4r-thermal:HeatProducer
										] ,
										[ rdf:type owl:Restriction ;
										  owl:onProperty b4r-thermal:nominalPower ;
										  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
										  owl:onClass b4r-thermal:HeatProducer
										] .


###  http://www.bim4ren.eu/domain/thermal#HeatStorage
b4r-thermal:HeatStorage rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty b4r-thermal:capacity ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass b4r-thermal:HeatStorage
                             ] .


###  http://www.bim4ren.eu/domain/thermal#HeatTransferElement
b4r-thermal:HeatTransferElement rdf:type owl:Class ;
							    rdfs:subClassOf [ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:airTightness ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#AirTightness>
												] ,
												[ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:infraredTransmittance ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
												] ,
												[ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:thermalMass ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
												] ,
												[ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:thermalResistance ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#ThermalResistance>
												] ,
												[ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:thermalTransmittance ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#ThermalTransmittance>
												] ,
												[ rdf:type owl:Restriction ;
												  owl:onProperty b4r-thermal:visibleTransmittance ;
												  owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
												] .


###  http://www.bim4ren.eu/domain/thermal#EfficiencyType
b4r-thermal:EfficiencyType rdf:type owl:Class ;
						   owl:equivalentClass [ rdf:type owl:Class ;
												 owl:oneOf ( b4r-thermal:CERTIFIED
                                                       b4r-thermal:MEASURED
                                                       b4r-thermal:DECLARED
															)
												] ;
						   rdfs:label "Type du rendement"@fr ,
									  "Tipo di rendimento"@it ,
                                      "Tipo de rendimiento"@es ,
                                      "Efficiency type"@en .


