@base <http://www.bim4ren.eu/domain/thermal> .

<http://www.bim4ren.eu/domain/thermal> rdf:type owl:Ontology ;
                                        owl:versionIRI <http://www.bim4ren.eu/domain/thermal/0.1> ;
                                        owl:imports <http://www.bim4ren.eu/property/0.1> .

#################################################################
#    Object Properties
#################################################################

###  http://www.bim4ren.eu/domain/thermal#airTightness
:airTightness rdf:type owl:ObjectProperty ;
              rdfs:label "air tighthness"@en ,
                         "ermeticità all'aria"@it ,
                         "estanqueidad"@es ,
                         "étanchéité à l'air"@fr .


###  http://www.bim4ren.eu/domain/thermal#capacity
:capacity rdf:type owl:ObjectProperty ;
          rdfs:label "Capacidad"@es ,
                     "Capacity"@en ,
                     "Capacité"@fr .


###  http://www.bim4ren.eu/domain/thermal#infraredTransmittance
:infraredTransmittance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/thermal#maxTemperature
:maxTemperature rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :setPoint ;
                rdfs:label "maximal temperature"@en ,
                           "température maximale"@fr .


###  http://www.bim4ren.eu/domain/thermal#minTemperature
:minTemperature rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :setPoint ;
                rdfs:label "minimal temperature"@en ,
                           "température minimale"@fr .


###  http://www.bim4ren.eu/domain/thermal#nominalPower
:nominalPower rdf:type owl:ObjectProperty ;
              rdfs:label "Nominal power"@en ,
                         "Puissance nominale"@fr ,
                         "Puissance utile"@fr .


###  http://www.bim4ren.eu/domain/thermal#setPoint
:setPoint rdf:type owl:ObjectProperty ;
          rdfs:label "consigne"@fr ,
                     "régulation"@fr ,
                     "setpoint"@en .


###  http://www.bim4ren.eu/domain/thermal#thermalEfficiency
:thermalEfficiency rdf:type owl:ObjectProperty ;
                   rdfs:label "Rendement"@fr ,
                              "Rendimento"@it ,
                              "Rendimiento térmico"@es ,
                              "Thermal efficiency"@en .


###  http://www.bim4ren.eu/domain/thermal#thermalMass
:thermalMass rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/thermal#thermalResistance
:thermalResistance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/thermal#thermalTransmittance
:thermalTransmittance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/thermal#visibleTransmittance
:visibleTransmittance rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://www.bim4ren.eu/domain/thermal#HeatProducer
:HeatProducer rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :thermalEfficiency ;
                                owl:someValuesFrom <http://www.bim4ren.eu/property#Ratio>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :maxTemperature ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :HeatProducer
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :minTemperature ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :HeatProducer
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :nominalPower ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :HeatProducer
                              ] .


###  http://www.bim4ren.eu/domain/thermal#HeatStorage
:HeatStorage rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :capacity ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :HeatStorage
                             ] .


###  http://www.bim4ren.eu/domain/thermal#HeatTransferElement
:HeatTransferElement rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty :airTightness ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#AirTightness>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :infraredTransmittance ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :thermalMass ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :thermalResistance ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#ThermalResistance>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :thermalTransmittance ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#ThermalTransmittance>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :visibleTransmittance ;
                                       owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                     ] .


