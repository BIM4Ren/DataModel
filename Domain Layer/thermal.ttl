                                                  owl:someValuesFrom bs:QuantityValue
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty b4r-thermal:airTightness ;
                                                  owl:allValuesFrom bs:QuantityValue
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty b4r-thermal:thermalMass ;
                                                  owl:allValuesFrom bs:QuantityValue
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty b4r-thermal:thermalResistance ;
                                                  owl:allValuesFrom bs:QuantityValue
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty b4r-thermal:thermalTransmittance ;
                                                  owl:allValuesFrom bs:QuantityValue
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty b4r-thermal:visibleTransmittance ;
                                                  owl:allValuesFrom bs:QuantityValue
                                                ] .


