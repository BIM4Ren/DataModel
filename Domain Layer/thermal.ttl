###  http://www.bim4ren.eu/domain/thermal#EfficiencyType
b4r-thermal:EfficiencyType rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Class ;
                                                 owl:oneOf ( b4r-thermal:CERTIFIED
                                                             b4r-thermal:DECLARED
                                                             b4r-thermal:MEASURED
                                                           )
                                               ] ;
                           rdfs:label "Efficiency type"@en ,
                                      "Tipo de rendimiento"@es ,
                                      "Tipo di rendimento"@it ,
                                      "Type du rendement"@fr .


###  http://www.bim4ren.eu/domain/thermal#HeatProducer
b4r-thermal:HeatProducer rdf:type owl:Class ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty b4r-thermal:thermalEfficiency ;
                                           owl:allValuesFrom bs:QuantityValue
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty b4r-thermal:maxTemperature ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass b4r-thermal:HeatProducer
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty b4r-thermal:minTemperature ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass b4r-thermal:HeatProducer
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty b4r-thermal:nominalPower ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass b4r-thermal:HeatProducer
                                         ] .


###  http://www.bim4ren.eu/domain/thermal#HeatStorage
b4r-thermal:HeatStorage rdf:type owl:Class .


###  http://www.bim4ren.eu/domain/thermal#HeatTransferElement
b4r-thermal:HeatTransferElement rdf:type owl:Class ;
