<http://www.bim4ren.eu/domain/fluid> rdf:type owl:Ontology ;
                                      owl:versionIRI <http://www.bim4ren.eu/domain/fluid/0.1> ;
                                      owl:imports <http://www.bim4ren.eu/property/0.1> .

#################################################################
#    Object Properties
#################################################################

###  http://www.bim4ren.eu/domain/fluid#HMTnoFlow
:HMTnoFlow rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :totalDynamicHead .


###  http://www.bim4ren.eu/domain/fluid#HMTnominalFlow
:HMTnominalFlow rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :totalDynamicHead .


###  http://www.bim4ren.eu/domain/fluid#capacity
:capacity rdf:type owl:ObjectProperty ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :capacity ;
                       owl:allValuesFrom <http://qudt.org/schema/qudt#QuantityValue>
                     ] ;
          rdfs:label "capacidad"@es ,
                     "capacity"@en ,
                     "capacité"@fr ,
                     "volume"@en ,
                     "volume"@fr ,
                     "volumen"@es .


###  http://www.bim4ren.eu/domain/fluid#infraredTransmittance
:infraredTransmittance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/fluid#maxPressure
:maxPressure rdf:type owl:ObjectProperty ;
             rdfs:label "maximal pressure"@en ,
                        "pression maximale"@fr .


###  http://www.bim4ren.eu/domain/fluid#nominalFlowRate
:nominalFlowRate rdf:type owl:ObjectProperty ;
                 rdfs:label "débit nominal"@fr ,
                            "nominal flow rate"@en .


###  http://www.bim4ren.eu/domain/fluid#thermalMass
:thermalMass rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/fluid#thermalResistance
:thermalResistance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/fluid#thermalTransmittance
:thermalTransmittance rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/fluid#totalDynamicHead
:totalDynamicHead rdf:type owl:ObjectProperty ;
                  rdfs:label "HMT"@fr ,
                             "Hauteur manométrique totale"@fr ,
                             "TDH"@en ,
                             "Total dynamic head"@en .


###  http://www.bim4ren.eu/domain/fluid#visibleTransmittance
:visibleTransmittance rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://www.bim4ren.eu/domain/fluid#FluidSegment
:FluidSegment rdf:type owl:Class ;
              rdfs:label "Segment de transfert de fluide"@fr .


###  http://www.bim4ren.eu/domain/fluid#FluidStorage
:FluidStorage rdf:type owl:Class ;
              rdfs:label "Elément de stockage de fluide"@fr .


###  http://www.bim4ren.eu/domain/fluid#FluidTerminal
:FluidTerminal rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :capacity ;
                                 owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                               ] ;
               rdfs:label "Terminal de fluide"@fr .


###  http://www.bim4ren.eu/domain/fluid#FluidTransferElement
:FluidTransferElement rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :HMTnoFlow ;
                                        owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :HMTnominalFlow ;
                                        owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :maxPressure ;
                                        owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :nominalFlowRate ;
                                        owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :totalDynamicHead ;
                                        owl:someValuesFrom <http://www.bim4ren.eu/property#Property>
                                      ] ;
                      rdfs:comment "An element that is used to transfer a fluid fro one point to another through a mechanical action. Typically, this includes pumps or circulators."@en ;
                      rdfs:label "Elément de transfert de fluide"@fr .


