###  http://www.bim4ren.eu/domain/spatial#length
:length rdf:type owl:ObjectProperty .

							   
###  http://www.bim4ren.eu/property#grossArea
:grossArea rdf:type owl:ObjectProperty ;
			  rdfs:subPropertyOf b4r-prop:area ;
			  rdfs:label "surface brut"@fr ,
                         "area lorda"@it ,
                         "área bruta"@es ,
                         "gross area"@en .  


###  http://www.bim4ren.eu/property#netArea
:netArea rdf:type owl:ObjectProperty ;
			     rdfs:subPropertyOf b4r-prop:area ;
				 rdfs:label "area netta"@it ,
							"surface net"@fr ,
							"net area"@en ,
							"área neta"@es .	


###  http://www.bim4ren.eu/domain/spatial#volume
:volume rdf:type owl:ObjectProperty ;
                   rdfs:domain :VolumeElement .


###  http://www.bim4ren.eu/domain/spatial#volumicMass
:volumicMass rdf:type owl:ObjectProperty ;
                        rdfs:domain :VolumeElement ;
                        rdfs:label "Masse volumique"@en ,
                                   "Volumic Mass"@en .


###  http://www.bim4ren.eu/domain/spatial#width
:width rdf:type owl:ObjectProperty .


