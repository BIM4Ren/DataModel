@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.bim4ren.eu/core/intervention> .

<http://www.bim4ren.eu/core/intervention> rdf:type owl:Ontology ;
                                           owl:versionIRI <http://www.bim4ren.eu/core/intervention/0.1> .

#################################################################
#    Object Properties
#################################################################

###  http://www.bim4ren.eu/core/intervention#associatedEvents
:associatedEvents rdf:type owl:ObjectProperty ;
                  owl:inverseOf :performedOn ;
                  rdfs:domain :Element ;
                  rdfs:range :Event .


###  http://www.bim4ren.eu/core/intervention#performed
:performed rdf:type owl:ObjectProperty ;
           owl:inverseOf :performedBy ;
           rdfs:domain :Organization ;
           rdfs:range :Event .


###  http://www.bim4ren.eu/core/intervention#performedBy
:performedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Event ;
             rdfs:range :Organization .


###  http://www.bim4ren.eu/core/intervention#performedOn
:performedOn rdf:type owl:ObjectProperty ;
             rdfs:domain :Event ;
             rdfs:range :Element .


#################################################################
#    Data properties
#################################################################

###  http://www.bim4ren.eu/core/intervention#endDate
:endDate rdf:type owl:DatatypeProperty .


###  http://www.bim4ren.eu/core/intervention#startDate
:startDate rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.bim4ren.eu/core/intervention#Checking
:Checking rdf:type owl:Class ;
          rdfs:subClassOf :Event .


###  http://www.bim4ren.eu/core/intervention#Element
:Element rdf:type owl:Class .


###  http://www.bim4ren.eu/core/intervention#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :performedBy ;
                         owl:someValuesFrom :Organization
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :performedOn ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Element
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :endDate ;
                         owl:someValuesFrom xsd:dateTime
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :startDate ;
                         owl:someValuesFrom xsd:dateTime
                       ] .


###  http://www.bim4ren.eu/core/intervention#Installation
:Installation rdf:type owl:Class ;
              rdfs:subClassOf :Event .


###  http://www.bim4ren.eu/core/intervention#Organization
:Organization rdf:type owl:Class .


###  http://www.bim4ren.eu/core/intervention#Repair
:Repair rdf:type owl:Class ;
        rdfs:subClassOf :Event .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Element
                :Event
                :Organization
              )
] .


